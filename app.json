{
  "expo": {
    "scheme": "acme",
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "name": "Портал МелГУ",
    "slug": "university-app",
    "version": "alpha-0.3.0",
    "android": {
      "package": "com.university.app",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "VIBRATE"
      ],
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "ios": {
      "bundleIdentifier": "com.university.app",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "11e1e134-159d-41fc-9eb0-8671c3006336"
      }
    },
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/11e1e134-159d-41fc-9eb0-8671c3006336"
    }
  }
}